import java.util.*;

public class Main
{
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
	
		    
		    String s;
		    s = sc.nextLine();
		    
		    char[] ch = new char[s.length()]; 
  
        // Copy character by character into array 
        for (int i = 0; i < s.length(); i++) { 
            ch[i] = s.charAt(i); 
        } 
  
        // Printing content of array 
        for (char c : ch) { 
            System.out.print(c+","); 
        } 

	}
}
